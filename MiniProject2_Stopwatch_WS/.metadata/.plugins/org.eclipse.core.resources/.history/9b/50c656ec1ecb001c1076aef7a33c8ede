#include "stopwatch.h"
#include "avr/io.h"

/* Configure the pins connected to 7447 decoder of common anode 7-segment
 * decoder_A connected to PC0
 * decoder_B connected to PC1
 * decoder_C connected to PC2
 * decoder_D connected to PC3
 * */
const SevenSegment_ConfigType SevenSegment_ConfigPin = {PC0,PC1,PC2,PC3};
